<div class="modal-cover"></div>
<div class="modal" data-modal="ticket-type">
    <ul class="box bg-light-gray">
        <% for (let ticket of tickets) { %>
        <li>
            <dl class="d-table">
                <dt class="d-td">
                    <%= ticket.ticket_name_kana %><br>
                    <span class="small-text"><%= ticket.ticket_note %></span>
                </dt>
                <dd class="d-td">
                    <div class="button blue-button ticket-type-select"><a href="#" data-ticket="<%= JSON.stringify(ticket) %>">選択</a></div>
                </dd>
            </dl>
        </li>
        <% } %>
    </ul>
</div>