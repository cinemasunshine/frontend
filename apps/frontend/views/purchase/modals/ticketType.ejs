<div class="modal-cover"></div>
<div class="modal" data-modal="ticket-type">
    <ul class="box">
        <% for (let salesTarget of sales.salesTargetList) { %>
        <li>
            <dl class="d-table">
                <dt class="d-td"><%= salesTarget.salesTargetName %></dt>
                <dd class="d-td">
                    <div class="button blue-button ticket-type-select"><a href="#" data-ticket-type="<%= salesTarget.salesTargetCode %>" data-ticket-price="1800">選択</a></div>
                </dd>
            </dl>
        </li>
        <% } %>
    </ul>
</div>