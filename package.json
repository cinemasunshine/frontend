    "connect-redis": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.3",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-ejs-layouts": "^2.3.0",
    "express-session": "^1.15.2",
    "express-validator": "^3.2.1",
    "fs-extra": "^3.0.1",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "i18n": "^0.8.3",
    "moment": "^2.18.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "winston": "^2.3.1"
  },
  "main": "index.js",
  "devDependencies": {
