{
  "name": "sskts-frontend",
  "version": "6.1.0",
  "scripts": {
    "ng": "ng",
    "build-client": "npm run build-client-development && npm run build-client-test && npm run build-client-production && node bin/rename dist/client/production/index.production.html dist/client/production/index.html",
    "build-client-local": "ng build --configuration=local",
    "build-client-development": "ng build --configuration=dev",
    "build-client-test": "ng build --configuration=test",
    "build-client-production": "ng build --configuration=production",
    "test-client": "ng test --code-coverage",
    "lint-client": "tslint --config tslint.json src/client/**/**.ts --exclude **/**.spec.ts --project src/client/tsconfig.json",
    "e2e": "ng e2e",
    "css-server": "npm run scss-server && postcss public/css/**/*.css -u autoprefixer -d public/css",
    "scss-server": "node-sass public/scss --output public/css",
    "start": "npm run start-server",
    "start-server": "node dist/server/index",
    "ssl": "node dist/server/ssl",
    "build-server": "tsc --project src/server --outDir dist/server --locale ja-jp",
    "lint-server": "tslint --config tslint.json src/server/**/**.ts --exclude **/**.spec.ts --project src/server/tsconfig.json",
    "build": "npm run css-server && npm run build-server && npm run build-client",
    "version": "git add -A",
    "preversion": "npm run build",
    "check": "npm run lint",
    "lint": "npm run lint-client && npm run lint-server",
    "test": "npm run check",
    "analyze": "webpack-bundle-analyzer dist/client/production/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.8",
    "@angular/common": "^8.2.8",
    "@angular/compiler": "^8.2.8",
    "@angular/core": "^8.2.8",
    "@angular/forms": "^8.2.8",
    "@angular/platform-browser": "^8.2.8",
    "@angular/platform-browser-dynamic": "^8.2.8",
    "@angular/router": "^8.2.8",
    "@motionpicture/coa-service": "^7.0.0",
    "@motionpicture/mvtk-reserve-service": "^1.3.0",
    "@motionpicture/sskts-api-javascript-client": "^8.0.1",
    "@motionpicture/sskts-api-nodejs-client": "^8.0.1",
    "aws-sdk": "^2.540.0",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.2.1",
    "debug": "^4.1.1",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.4.0",
    "express-session": "^1.16.2",
    "express-validator": "^4.3.0",
    "helmet": "^3.21.1",
    "http-status": "^1.3.2",
    "i18n": "^0.8.3",
    "libphonenumber-js": "^1.7.25",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "qrcode": "^1.4.2",
    "redis": "^2.8.0",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.5.3",
    "stream": "0.0.2",
    "timers": "^0.1.1",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "webpack-bundle-analyzer": "^3.5.2",
    "winston": "^2.4.4",
    "xml-js": "^1.6.11",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "^8.3.6",
    "@angular/compiler-cli": "^8.2.8",
    "@angular/language-service": "^8.2.8",
    "@types/basic-auth": "^1.1.2",
    "@types/body-parser": "^1.17.1",
    "@types/connect-redis": "0.0.11",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "4.1.5",
    "@types/ejs": "^2.6.3",
    "@types/express": "^4.17.1",
    "@types/express-ejs-layouts": "^2.3.2",
    "@types/express-session": "^1.15.14",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "0.0.44",
    "@types/http-status": "^0.2.30",
    "@types/i18n": "^0.8.6",
    "@types/jasmine": "^3.4.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^8.10.54",
    "@types/qrcode": "^1.3.4",
    "@types/redis": "^2.8.14",
    "@types/uuid": "^3.4.5",
    "@types/winston": "^2.3.9",
    "autoprefixer": "^9.6.1",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "node-sass": "^4.13.0",
    "nyc": "^14.1.1",
    "postcss": "^7.0.18",
    "postcss-cli": "^6.1.3",
    "protractor": "^5.4.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
