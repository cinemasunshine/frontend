    "@types/fs-extra": "0.0.35",
    "@types/helmet": "0.0.33",
    "@types/http-status": "^0.2.29",
    "@types/i18n": "^0.5.20",
    "@types/jquery": "^2.0.34",
    "@types/log4js": "0.0.32",
    "@types/mocha": "^2.2.40",
    "@types/moment": "^2.13.0",
    "@types/request": "0.0.36",
    "@types/request-promise-native": "^1.0.2",
    "@types/selenium-webdriver": "^2.53.39",
    "@types/supertest": "^2.0.0",
    "autoprefixer": "^6.7.2",
    "cssnano": "^3.10.0",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-jsdoc": "^2.1.0",
    "ink-docstrap": "^1.3.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.1",
