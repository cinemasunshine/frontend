{
  "name": "sskts-frontend",
  "version": "1.6.1",
  "private": true,
  "scripts": {
    "check": "npm run tslint && nsp check",
    "snyk": "snyk auth && snyk test --ignore-policy",
    "start": "node ./index.js",
    "ui-test": "node ./test/uiTest.js",
    "css": "npm run scss && postcss -c ./.postcssrc.json",
    "scss": "node-sass public/scss --output public/css",
    "tslint": "tslint -c tslint.json app/**/**.ts libs/MP/**.ts --type-check --project tsconfig.json",
    "tslint-fix": "tslint -c tslint.json app/**/**.ts --project tsconfig.json --fix",
    "jsdoc": "grunt jsdoc",
    "clean": "rimraf libs/**/*.js app/**/*.js test/**/*.js docs/* npm-debug.log*",
    "build": "tsc",
    "test": "npm run check && mocha --no-timeouts --recursive",
    "preversion": "npm run clean && npm run build && npm run css && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags"
  },
  "dependencies": {
    "@motionpicture/coa-service": "^4.0.0",
    "@motionpicture/gmo-service": "^4.0.0",
    "@motionpicture/mvtk-service": "^2.0.2",
