{
  "name": "sskts-frontend",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "development": "bin\\env\\development.bat && bin\\env\\debug.bat && node ./index.js",
    "start": "node ./index.js",
    "test": "bin\\env\\development.bat && mocha ./test/mocha/**/*.js --no-timeouts --recursive",
    "ui-test": "node ./test/uiTest.js",
    "postcss": "postcss -c ./.postcssrc.json",
    "css": "npm run scss && npm run postcss",
    "scss": "node-sass public/scss/import.scss public/scss/style.css",
    "scss-w": "node-sass public/scss/import.scss public/css/style.css -w",
    "tslint": "tslint -c tslint.json apps/**/**.ts libs/MP/**.ts --type-check --project tsconfig.json",
    "tslint-fix": "tslint -c tslint.json apps/**/**.ts --project tsconfig.json --fix",
    "tsc": "tsc -p ./ -w",
    "jsdoc": "grunt jsdoc",
    "clean": "rimraf libs/**/*.js apps/**/*.js test/**/*.js docs/**/*",
    "build": "npm run nsp && npm run clean && npm run tslint && tsc -p ./ && npm run test && npm run css && npm run jsdoc",
    "nsp": "nsp check --output summary"
  },
  "dependencies": {
    "@motionpicture/coa-service": "^2.0.6",
    "@motionpicture/gmo-service": "^2.0.10",
    "@motionpicture/mvtk-service": "^2.0.2",
    "basic-auth": "^1.1.0",
    "body-parser": "^1.15.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.1",
    "ejs": "^2.5.6",
    "express": "^4.14.0",
    "express-ejs-layouts": "^2.3.0",
    "express-session": "^1.14.2",
    "express-validator": "^3.1.2",
    "fs-extra": "^1.0.0",
    "helmet": "^3.1.0",
    "http-status": "^1.0.1",
    "i18n": "^0.8.3",
    "log4js": "^1.0.1",
    "moment": "^2.17.1",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3"
  },
  "main": "index.js",
  "devDependencies": {
    "@types/basic-auth": "0.0.31",
    "@types/body-parser": "0.0.33",
    "@types/config": "0.0.30",
    "@types/connect-redis": "0.0.5",
    "@types/cookie-parser": "^1.3.30",
    "@types/debug": "0.0.29",
