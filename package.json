{
  "name": "sskts-frontend",
  "version": "3.1.1",
  "scripts": {
    "ng": "ng",
    "build-client-local": "ng build --environment=local --output-path=./dist/client/development --aot",
    "build-client": "npm run build-client-development && npm run build-client-test && npm run build-client-production",
    "build-client-development": "ng build --environment=dev --output-path=./dist/client/development --aot",
    "build-client-test": "ng build --environment=test --output-path=./dist/client/test --aot",
    "build-client-production": "ng build --environment=prod --output-path=./dist/client/production --aot",
    "test-client": "ng test --code-coverage",
    "lint-client": "tslint --config tslint.json src/client/**/**.ts --exclude **/**.spec.ts --project src/client/tsconfig.json",
    "e2e": "ng e2e",
    "css-server": "npm run scss-server && postcss dist/server/public/css/**/*.css -u autoprefixer -d dist/server/public/css",
    "scss-server": "node-sass src/server/public/scss --output dist/server/public/css",
    "start": "npm run start-server",
    "start-server": "node dist/server/index",
    "ssl": "node dist/server/ssl",
    "build-server": "tsc --project src/server --outDir dist/server --locale ja-jp",
    "lint-server": "tslint --config tslint.json src/server/**/**.ts --exclude **/**.spec.ts --project src/server/tsconfig.json",
    "test": "npm run lint",
    "build": "npm run css-server && npm run build-server && npm run build-client",
    "version": "git add -A",
    "preversion": "npm run build",
    "lint": "npm run lint-client && npm run lint-server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.5",
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
    "@angular/core": "^5.2.5",
    "@angular/forms": "^5.2.5",
    "@angular/http": "^5.2.5",
    "@angular/platform-browser": "^5.2.5",
    "@angular/platform-browser-dynamic": "^5.2.5",
    "@angular/router": "^5.2.5",
    "@motionpicture/coa-service": "^4.0.9",
    "@motionpicture/mvtk-reserve-service": "^1.2.0",
    "@motionpicture/sskts-api-javascript-client": "^3.0.0",
    "@motionpicture/sskts-api-nodejs-client": "^2.1.2",
    "aws-sdk": "^2.195.0",
    "basic-auth": "^2.0.0",
    "body-parser": "^1.18.2",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.3",
    "debug": "^3.1.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-ejs-layouts": "^2.3.1",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "helmet": "^3.11.0",
    "http-status": "^1.0.1",
    "i18n": "^0.8.3",
    "libphonenumber-js": "^1.0.24",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "redis": "^2.8.0",
    "rxjs": "^5.5.6",
    "uuid": "^3.2.1",
    "winston": "^2.4.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.8",
    "@angular/compiler-cli": "^5.2.5",
    "@angular/language-service": "^5.2.5",
    "@types/basic-auth": "^1.1.2",
    "@types/body-parser": "^1.16.8",
    "@types/connect-redis": "0.0.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.30",
    "@types/ejs": "^2.5.0",
    "@types/express": "^4.11.0",
    "@types/express-ejs-layouts": "^2.3.1",
    "@types/express-session": "^1.15.8",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "0.0.37",
    "@types/http-status": "^0.2.30",
    "@types/i18n": "^0.8.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/moment-timezone": "^0.5.4",
    "@types/node": "^9.4.6",
    "@types/redis": "^2.8.5",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.8",
    "autoprefixer": "^7.2.6",
    "codelyzer": "^4.1.0",
    "jasmine-core": "~2.9.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nsp": "^3.1.0",
    "nyc": "^11.5.0",
    "postcss": "^6.0.16",
    "postcss-cli": "^4.1.1",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
